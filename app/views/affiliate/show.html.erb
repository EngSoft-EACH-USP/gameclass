<p id="notice"><%= notice %></p>

<h1>Affiliates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @affiliates.each do |affiliate| %>
      <tr>
        <td><%= affiliate.user.name %></td>
        <td><%= affiliate.description %></td>
        <td><%= affiliate.status %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

